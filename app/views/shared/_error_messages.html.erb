<% if errors.any? %>
  <div id='error_explanation' class='alert alert-danger'>
      <p>
      <%= ti(:"errors.header", count: errors.count, model: object.to_s) %>
      </p>
      <ul>
          <% errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
      </ul>
  </div>
<% end -%>
